{
  "name": "@sandwich-go/log-viewer",
  "version": "0.1.12",
  "main": "dist/log-viewer.umd.js",
  "module": "dist/log-viewer.esm.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "stdver": "standard-version -m '[skip ci] chore(release): v%s'",
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/log-viewer.esm.js",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/log-viewer.umd.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/log-viewer.min.js"
  },
  "dependencies": {
    "vue-virtual-scroll-list": "1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-loader": "^8.0.5",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "husky": "1.3.1",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.0",
    "minimist": "^1.2.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.4.1",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "standard-version": "^6.0.1",
    "stylelint": "^9.10.0",
    "stylelint-config-standard": "^18.2.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-styleguidist": "^3.16.3",
    "vue-template-compiler": "2.6.10",
    "webpack": "^4.29.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
